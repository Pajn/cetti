{
  "name": "cetti",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm-run-all postinstall:*",
    "postinstall:typings": "tsd install",
    "postinstall:ts_base_config": "cp tsconfig.base.json tsconfig.json",
    "postinstall:typings_custom": "cp -a typings_custom/* typings/",
    "postinstall:tsconfig": "tsconfig -i 2",
    "start": "npm run dev",
    "lint": "tslint --rules-dir tools/lint-rules `find app -name '*.ts' -o -name '*.tsx'`",
    "build": "rm -rf dist && NODE_ENV=production webpack",
    "build_dev": "webpack",
    "build_typescript": "tsc",
    "dev": "webpack-dev-server --hot --inline --port 3000",
    "clean": "rm -rf .tmp"
  },
  "dependencies": {
    "history": "^1.13.0",
    "material-ui": "^0.13.2",
    "radium": "^0.15.3",
    "ramda": "^0.18.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-mdl": "^1.0.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "rx": "^4.0.6"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.1.1",
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.1.18",
    "babel-preset-stage-1": "^6.0.15",
    "babel-runtime": "^6.0.14",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.4",
    "jest-cli": "^0.7.0",
    "npm-run-all": "^1.2.12",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.6.0",
    "tsconfig-glob": "^0.2.1",
    "tsd": "^0.6.5",
    "tslint": "^2.5.1",
    "typescript": "^1.7.0-dev.20151016",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "homepage": "https://github.com/drager/cetti#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drager/cetti.git"
  },
  "bugs": {
    "url": "https://github.com/drager/cetti/issues"
  }
}
